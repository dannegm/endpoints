        "@mozilla/readability": "^0.5.0",
        "@react-email/components": "^0.0.25",
        "@supabase/supabase-js": "^2.45.4",
        "axios": "^1.7.7",
        "babel-plugin-module-resolver": "^5.0.2",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "got": "^11.8.2",
        "jsdom": "^25.0.1",
        "metascraper": "^5.45.23",
